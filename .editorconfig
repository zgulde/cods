[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 2

[*.md]
indent_style = space
indent_size = 4
tab_width = 4

[*.sh]
indent_style = tab
indent_size = 4
tab_width = 4
[server]
indent_style = tab
indent_size = 4
tab_width = 4
